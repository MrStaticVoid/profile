<% require 'etc'; unless Etc.getlogin == 'jtl' -%>
AddKeysToAgent yes
CanonicalizeHostname yes
CanonicalDomains nest home umd.edu

<% end -%>
# Git hosts
Host gitlab.james.tl gitlab.umd.edu github.com
    ForwardX11 no

# My stuff
Host *.nest
    User james
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    ServerAliveInterval 60

# Work stuff
Host *.umd.edu *.internal
    User jtl
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    PreferredAuthentications gssapi-with-mic,keyboard-interactive,password
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    ServerAliveInterval 60

# Defaults
Host *
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    ServerAliveInterval 60
