AddKeysToAgent yes
CanonicalizeHostname yes
CanonicalDomains nest home localdomain

# Git hosts
Host github.com gitlab.localhost
    ForwardX11 no

Host gitlab.james.tl
    ForwardX11 no
    Hostname falcon.nest
    Port 2222

# My stuff
Host *.nest
    User james
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    ServerAliveInterval 60

# Defaults
Host *
    ForwardAgent yes
    ForwardX11 yes
    ServerAliveInterval 60
