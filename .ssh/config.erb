AddKeysToAgent yes
CanonicalizeHostname yes
CanonicalDomains nest home localdomain

# Git hosts
Host gitlab.james.tl github.com gitlab.localhost
    ForwardX11 no

# My stuff
Host *.nest
    User james
    ForwardAgent yes
    ForwardX11 yes
    ForwardX11Trusted yes
    ServerAliveInterval 60

# Defaults
Host *
    ForwardAgent yes
    ForwardX11 yes
    ServerAliveInterval 60
