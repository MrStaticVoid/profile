<% unless @fqdn =~ /\.umd\.edu$/ -%>
CanonicalizeHostname yes
CanonicalDomains nest home umd.edu

<% end -%>
# Routers
Host prey.heloandnala.net prey.home
    User admin
    Port 2221
    ForwardX11 no
Host bridge.home
    User admin
    ForwardX11 no
Host bushytail.thestaticvoid.com bushytail.home
    User admin
    Port 2221
    ForwardX11 no
Host foyer.home
    User admin
    ForwardX11 no

# Phone
Host motox.nest
    User root
    Port 2222
    ForwardX11 no

# GitLab
Host gitlab.umd.edu
    ForwardX11 no

# My stuff
Host *.nest
    User james
    ForwardX11 yes
    ForwardX11Trusted yes

# Work stuff
Host *.umd.edu
    User jtl
    ForwardX11 yes
    ForwardX11Trusted yes
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes

# Defaults
Host *
    ForwardAgent yes
    ServerAliveInterval 60
