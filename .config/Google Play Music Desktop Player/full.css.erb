<% require 'colorscheme'
dark = ColorScheme.dark.colors_by_base -%>
#material-app-bar {
  background-color: "<%= dark[0x01].web %>";
}

#material-app-bar .tab-text,
[data-id="prev-history"],
[data-id="next-history"],
#searchIcon {
  color: "<%= dark[0x05].web %>";
}

sj-search-box,
sj-search-box[opened],
sj-search-box[has-query] {
  background-color: "<%= dark[0x00].web %>";
}

sj-search-box #input.sj-search-box {
  color: "<%= dark[0x05].web %>";
}

sj-search-box[opened]:not([num-suggestions="0"]) #input.sj-search-box {
  border-bottom-color: "<%= dark[0x08].web %>";
}

sj-search-suggestion:hover,
sj-search-suggestion.query-selected {
  background-color: "<%= dark[0x08].web %>";
}

sj-search-suggestion #queryText.sj-search-suggestion .sub-match.sj-search-suggestion {
  color: "<%= dark[0x05].web %>";
}

#material-one-left #left-nav-open-button svg {
  fill: "<%= dark[0x05].web %>";
}

body {
  background-color: "<%= dark[0x08].web %>";
}

#drawer, .material-card {
  background-color: "<%= dark[0x00].web %>";
}

#nav-container,
#loading-progress,
#loading-progress-bar {
  background: "<%= dark[0x00].web %>";
}

#loading-overlay {
  background: rgba(<%= dark[0x00].rgb.join(', ') %>, 0.7);
}

#loading-progress-bar {
  border: none;
}

.nav-toolbar,
.material-card .details,
.material-card .image-wrapper,
.situations-filter {
  background: "<%= dark[0x00].web %>";
}

#nav {
  background: "<%= dark[0x00].web %>";
  color: "<%= dark[0x05].web %>";
}

#player,
.player-rating-container,
#player.material .now-playing-actions paper-icon-button {
  color: "<%= dark[0x05].web %>";
  background-color: "<%= dark[0x00].web %>";
}

#player.material paper-icon-button.playing > iron-icon::before {
  content: '';
  display: block;
  position: absolute;
  background: "<%= dark[0x05].web %>";
  z-index: -1;
  border-radius: 100%;
  height: calc(100% - 16px);
  width: calc(100% - 16px);
  top: 8px;
  left: 8px;
}

#player.material:hover #material-player-progress #sliderContainer:not(.disabled) #sliderBar #progressContainer,
.playlist-view .editable:hover {
  background: "<%= dark[0x08].web %>";
}

.cluster-text-protection::before,
.cluster-text-protection {
  background: rgba(0, 0, 0, 0);
}

.title,
.situation-title,
.recommended-header {
  color: "<%= dark[0x05].web %>";
}

.nav-item-container {
  color: "<%= dark[0x05].web %>";
}

.nav-item-container .owner-name {
  color:  "<%= dark[0x05].shade(40).web %>";
}

#nav_collections .nav-item-container:focus,
.nav-item-container:focus,
.nav-item-container:hover,
.nav-item-container.selected {
  background-color: "<%= dark[0x08].web %>";
}

.fade-out:after {
  display: none;
}

.column.col-0 .material-card:first-child .image-wrapper {
  padding-top: 0;
}

/*
* TODO: Add an option to gmusic-theme.js that handles the difference between
*       light and dark themes.  The image below should be for "dark" themes only
*/
.song-row .song-indicator {
  background-image: url('ani_equalizer_white_x2.gif');
  background-size: 28px 28px;
  background-color: "<%= dark[0x00].web %>";
}

.song-row:hover .song-indicator {
  background-color: "<%= dark[0x08].web %>";
}

.material-detail-view .has-hero-image,
.song-row td,
.song-row td > *,
.song-row.currently-playing td,
.song-row.currently-playing td > *,
.upload-progress-row td,
.song-row.selected-song-row td {
  background-color: "<%= dark[0x00].web %>";
  color: "<%= dark[0x05].web %>";
}

.song-row.selected-song-row .song-indicator,
.song-row.selected-song-row .title-right-items,
.song-row.selected-song-row .content,
.song-row.selected-song-row .song-indicator[data-playback-status="paused"],
.song-row.selected-song-row .song-indicator[data-playback-status="loading"],
.song-row.selected-song-row [data-col="index"] .hover-button[data-id="play"],
.song-row.selected-song-row [data-col="track"] .hover-button[data-id="play"],
.song-row.selected-song-row td,
.song-row.selected-song-row td > * {
  background-color: "<%= dark[0x08].web %>";
}

.song-row:hover,
.song-row.hover .song-indicator,
.song-row.hover .title-right-items,
.song-row.hover .content,
.song-row.hover .song-indicator[data-playback-status="paused"],
.song-row.hover .song-indicator[data-playback-status="loading"],
.song-row.hover [data-col="index"] .hover-button[data-id="play"],
.song-row.hover [data-col="track"] .hover-button[data-id="play"],
.song-row.hover td,
.song-row.hover td > * {
  background-color: "<%= dark[0x08].web %>";
}

.song-row [data-col="radio"],
.song-row [data-col="radio"] svg,
.song-row.hover [data-col="radio"],
.song-row.hover [data-col="radio"] svg,
.song-row.selected-song-row [data-col="radio"],
.song-row.selected-song-row [data-col="radio"] svg, {
