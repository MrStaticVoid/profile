<% require 'colorscheme'
c = ColorScheme.new
colors = c.terminal
colors.take(8).each_with_index do |color, i| -%>
[Color<%= i %>]
Color=<%= color.rgb.join(',') %>

[Color<%= i %>Faint]
Color=<%= color.shade(25).rgb.join(',') %>

[Color<%= i %>Intense]
Color=<%= colors[i + 8].rgb.join(',') %>

<% end -%>
[Background]
Color=<%= c.background.rgb.join(',') %>

[BackgroundFaint]
Color=<%= c.background.shade(25).rgb.join(',') %>

[BackgroundIntense]
Color=<%= c.background.rgb.join(',') %>

[Foreground]
Color=<%= c.foreground.rgb.join(',') %>

[ForegroundFaint]
Color=<%= c.foreground.shade(25).rgb.join(',') %>

[ForegroundIntense]
Color=<%= c.foreground.rgb.join(',') %>

[General]
Description=Mine
Opacity=1
Wallpaper=
