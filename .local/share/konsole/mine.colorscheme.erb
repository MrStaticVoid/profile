<% require 'colorscheme'
cs = ColorScheme.new
colors = cs.terminal
colors.take(8).each do |c| -%>
[Color<%= c.ansi %>]
Color=<%= c.rgb.join(',') %>

[Color<%= c.ansi %>Faint]
Color=<%= c.shade(33).rgb.join(',') %>

[Color<%= c.ansi %>Intense]
Color=<%= colors[c.ansi + 8].rgb.join(',') %>

<% end -%>
[Background]
Color=<%= cs.background.rgb.join(',') %>

[BackgroundFaint]
Color=<%= cs.background.shade(33).rgb.join(',') %>

[BackgroundIntense]
Color=<%= cs.background.rgb.join(',') %>

[Foreground]
Color=<%= cs.foreground.rgb.join(',') %>

[ForegroundFaint]
Color=<%= cs.foreground.shade(33).rgb.join(',') %>

[ForegroundIntense]
Color=<%= cs.foreground.rgb.join(',') %>

[General]
Description=Mine
Opacity=1
Wallpaper=
