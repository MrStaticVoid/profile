[Appearance]
ColorScheme=base16-bright
Font=Monospace,9,-1,0,50,0,0,0,0,0

[General]
Environment=TERM=xterm-256color
LocalTabTitleFormat=%w
Name=Shell
Parent=FALLBACK/
RemoteTabTitleFormat=%w
SaveGeometryOnExit=false
